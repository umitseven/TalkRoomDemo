@model List<TalkRoomDemo.DtoLayer.Dtos.ServerListDto>


    @if(Model != null && Model.Any())
    {
        @foreach (var item in Model)
        {
            <a href="@Url.Action("Details", "AddRoom", new { id = item.ServerID })">

                <img class="serverChanel" src="@item.ServerImageUrl" title="@item.ServerName"/>

            </a>
        }
    }
    else
    {
        
    }